<!DOCTYPE html>
<html>
<head>
  <title>Geofotr</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if flash[:errors] %>
  <%= flash[:errors] %>
<% end %>

<% if current_user %>
  <%= "What up, #{current_user.username}?" %>
  <%= render "shared/signout" %>
<% else %>
  <%= link_to "Sign Up", new_user_url %>
  <%= link_to "Sign In", new_session_url %>
<% end %>

<%= yield %>

</body>
</html>
